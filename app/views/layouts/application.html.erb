<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= content_for?(:title) ? yield(:title) : "Ayuda Estudiantil" %></title>
    <meta name="description" content="">
    <meta name="author" content="">
    <%= stylesheet_link_tag "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <%= yield(:head) %>
  </head>
  <body>
    <header class="navbar navbar-fixed-top">
      <nav class="navbar-inner">
        <div class="container">
          <%= render 'layouts/navigation' %>
        </div>
      </nav>
    </header>
    <div id="main" role="main">
      <div class="container">
        <div class="content">
           <div class="row">
            <div class="span12">
              <%= render 'layouts/messages' %>
              <%= yield %>
            </div>
          </div>
          <footer>

           
            
          </footer>
        </div>
      </div> <!--! end of .container -->
    </div> <!--! end of #main -->
    <br>
     <pre>
  <% idUser = session[:user_id]  %>
  <% if  idUser == nil %>
  <%= "Tipo usuario: visita" %> 
  <% else %>      
      <% identity =  Identity.find(idUser) %>          
      <% if  identity.administrator %>
          <%= "Tipo usuario: Admin - Nombre:  "+ identity.name + " - Email: "+identity.email %>   
      <% else %>
          <%= "Tipo usuario: Normal - Nombre: "+ identity.name + " - Email: "+identity.email%>           
      <% end %>
  <% end %>
</pre> 
  </body>
</html>